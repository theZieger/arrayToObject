  "repository": {
    "type": "git",
    "url": "git+https://github.com/theZieger/arrayToObject.git"
  },
  "author": "thezieger",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/theZieger/arrayToObject/issues"
  },
  "homepage": "https://github.com/theZieger/arrayToObject#readme",
  "devDependencies": {
    "jasmine": "^2.6.0",
    "umd": "^3.0.1"
  }
}
