{
  "name": "arr-to-object",
  "version": "0.3.0",
  "description": "Easily convert an array to an object.",
  "main": "dist/arrayToObject.js",
  "scripts": {
    "test": "jasmine",
    "build": "npm test && node_modules/umd/bin/cli.js arrayToObject src/arrayToObject.js dist/arrayToObject.js -c"
